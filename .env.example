# LLM Provider: "anthropic" (default) | "openai" | "openai-compatible"
LLM_PROVIDER=anthropic

# Anthropic (default — powers all AI agents)
ANTHROPIC_API_KEY=

# Model override (defaults to claude-sonnet-4-20250514)
# LLM_MODEL=claude-sonnet-4-20250514

# OpenAI (if using GPT instead)
# OPENAI_API_KEY=

# OpenAI-compatible endpoint (if using custom provider)
# LLM_BASE_URL=

# Perplexity Sonar (optional — for research features)
PERPLEXITY_API_KEY=

# Browserbase + Stagehand (optional — for web scraping)
BROWSERBASE_API_KEY=
BROWSERBASE_PROJECT_ID=
